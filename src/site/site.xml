<project xmlns="http://maven.apache.org/DECORATION/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/DECORATION/1.0.0 http://maven.apache.org/xsd/decoration-1.0.0.xsd">

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.5</version>
    </skin>

    <bannerRight>
        <name>avatar</name>
        <src>images/my-avatar-80.png</src>
    </bannerRight>

    <custom>
        <fluidoSkin>
            <topBarEnabled>false</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>

            <leftColumnClass>span3</leftColumnClass>
            <bodyColumnClass>span9</bodyColumnClass>
        </fluidoSkin>
    </custom>

    <body>
        <head>
            <![CDATA[<link rel="stylesheet" href="./css/site.css" type="text/css" />]]>
        </head>

        <menu name="Overview">
            <item name="Introduction" href="index.html"/>
            <item name="Plugin goals" href="play2-maven-plugin/plugin-info.html"/>
        </menu>

        <menu name="Usage">
            <item name="General instructions" href="usage.html"/>
            <item name="Play 2.6.x" href="usage-play26.html"/>
            <item name="Play 2.5.x" href="usage-play25.html"/>
            <item name="Play 2.4.x" href="usage-play24.html"/>
            <item name="Play 2.3.x" href="usage-play23.html"/>
            <item name="Play 2.2.x" href="usage-play22.html"/>
            <item name="Play 2.1.x" href="usage-play21.html"/>
        </menu>

        <menu name="Migration">
            <item name="From previous plugin version" href="migration-plugin.html"/>
            <item name="From Play 2.5.x to 2.6.x" href="migration-play26.html"/>
            <item name="From Play 2.4.x to 2.5.x" href="migration-play25.html"/>
            <item name="From Play 2.3.x to 2.4.x" href="migration-play24.html"/>
            <item name="From Play 2.2.x to 2.3.x" href="migration-play23.html"/>
            <item name="From Play 2.1.x to 2.2.x" href="migration-play22.html"/>
        </menu>

        <menu name="IDE integration">
            <item name="Eclipse/ScalaIDE" href="ide-integration-eclipse.html"/>
            <item name="IntelliJ IDEA" href="ide-integration-idea.html"/>
        </menu>

        <menu name="Other topics">
            <item name="Running in DEV mode" href="running.html"/>
            <item name="Multi-module builds" href="multi-module-builds.html"/>
            <item name="WAR packaging" href="war-packaging.html"/>
        </menu>

        <menu ref="parent" inherit="top"/>
        <menu ref="modules" inherit="bottom"/>
        <menu ref="reports" inherit="bottom"/>
    </body>

    <googleAnalyticsAccountId>${googleAnalyticsAccountId}</googleAnalyticsAccountId>

</project>
